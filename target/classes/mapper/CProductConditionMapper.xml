            </if>
            <if test="liRun != null and liRun != ''">
                li_run = #{liRun},
            </if>
            <if test="jingouNum != null">
                jingou_num = #{jingouNum},
            </if>
        </set>
        <where>
            <if test="productName != null and productName != ''">
                and product_name = #{productName}
            </if>
            <if test="productPrice != null and productPrice != ''">
